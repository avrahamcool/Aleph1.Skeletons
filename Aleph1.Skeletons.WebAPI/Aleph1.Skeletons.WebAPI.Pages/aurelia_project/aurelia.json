{
	"name": "pages",
	"type": "project:application",
	"paths": {
		"root": "src",
		"resources": "resources",
		"elements": "resources/elements",
		"attributes": "resources/attributes",
		"valueConverters": "resources/value-converters",
		"bindingBehaviors": "resources/binding-behaviors"
	},
	"transpiler": {
		"id": "typescript",
		"fileExtension": ".ts"
	},
	"build": {
		"options": {
			"server": "dev",
			"coverage": false
		}
	},
	"platform": {
		"hmr": false,
		"open": true,
		"port": 8080,
		"host": "localhost",
		"output": "dist"
	}
}