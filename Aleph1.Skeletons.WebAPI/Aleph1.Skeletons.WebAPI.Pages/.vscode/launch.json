{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8080",
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}/src",
			"userDataDir": "${workspaceRoot}/.chrome",
			"sourceMapPathOverrides": {
				"webpack:///./src/*": "${webRoot}/*"
			}
		}
	]
}